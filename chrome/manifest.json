{
  "manifest_version": 3,
  "name": "No Nitro, No Cry",
  "description": "An extension that lets you use Discord emojis anywhere â€” in any chat, on any server. Enjoy both animated and static emojis without Nitro, completely free. Some features powered with Not Quite Nitro.",
  "version": "1.5.2",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    }
  },
  "icons": {
    "16": "/images/16.png",
    "32": "/images/32.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://discord.com/*"
      ],
      "js": [
        "jquery.min.js",
        "script.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ]
}
